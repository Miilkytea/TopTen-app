<h1>Lists#edit</h1>
<h2>Edit Lists</h2>

<form class="list" id="edit-list" method "POST" action="users/lists/<%= @list.id %>">
  <input type="hidden" name="_method" value="PUT">

  <div>
    <label for="book-title">Title</label>
    <input type='text' name='list[title]' id="list-title" value="<%= @list.title%>">
  </div> 

  <div>
    <input type="submit" value"Save List" class='btn'>
  </div>
</form>  